{
  "buildCommand": "npm run build",
  "functions": {
    "app/api/shorten/route.ts": {
      "maxDuration": 10
    },
    "app/[shortCode]/page.tsx": {
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1",
      "permanent": false
    }
  ]
}
